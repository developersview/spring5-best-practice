<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="student0" class="com.developersview.ioc.Student">
        <property name="studentName">
            <value>Pranoy</value>
        </property>
    </bean>

    <bean id="student1" class="com.developersview.ioc.Student">
        <property name="studentName">
            <value>Martha</value>
        </property>
    </bean>

    <bean id = "employee0" class="com.developersview.di.Employees">
        <constructor-arg name="name" value="Walter O'Brien "/>
        <constructor-arg name="iq" value="197"/>
        <constructor-arg name="skill" value="Ultimate Genius"/>
    </bean>
</beans>